[EFD_WaterProofSolution.AddReg]
HKR,"ElanFP\WaterProofSolution",SupportWaterProofSolution,%REG_DWORD%,0		; 0 = No Support, 1 = Support
HKR,"ElanFP\WaterProofSolution",WaterProof_Level1_Threshold,%REG_DWORD%,40
